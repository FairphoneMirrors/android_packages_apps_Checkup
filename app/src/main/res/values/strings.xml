    <!-- LED -->
    <string name="led_test_title">LED</string>
    <string name="led_test_description">This test checks if the LED is working. Press \'start test\' and make sure you see the LED blinking.</string>
    <string name="led_test_finish_question">You should see the LED lighting up. Is the LED blinking?</string>

    <!-- Proximity sensor -->
    <string name="proximity_test_title">Proximity sensor</string>
    <string name="proximity_test_description">This test tests the proximity sensor. Press \'start test\' and cover the top half of the Fairphone completely with your hand.</string>
    <string name="proximity_state_triggered">Triggered</string>
    <string name="proximity_state_not_triggered">Not triggered</string>

    <!-- Ambient light -->
    <string name="ambient_light_sensor_test_title">Ambient light sensor</string>
    <string name="ambient_light_sensor_test_description">This test checks if the ambient light sensor is working properly. Repeatedly cover the top half of your Fairphone, the value read by the sensor should change.</string>
    <string name="illuminance">Illuminance: </string>
    <string name="illuminance_unit">lx</string>

    <!-- Ear speaker -->
    <string name="ear_speaker_test_title">Ear speaker</string>
    <string name="ear_speaker_test_description">This test checks if the ear speaker is working. Press \'start test\' and make sure you hear the music play.</string>
    <string name="ear_speaker_test_finish_question">You should hear some music playing. Is the ear speaker working?</string>

    <!-- Front camera -->
    <string name="front_camera_test_title">Front camera</string>

    <!-- Secondary microphone -->
    <string name="secondary_mic_test_title">Secondary microphone</string>
    <string name="secondary_mic_test_description">This will test the noise cancellation microphone. Press \'start test\' and pretend you are making a phone call, but hold your phone upside down. Can you hear yourself?</string>
    <string name="secondary_mic_test_finish_question">Talk to your Fairphone. Can you hear yourself?</string>


    <!-- Bottom module -->
    <string name="bottom_module_name">Bottom module</string>
    <string name="bottom_module_description">Vibration motor, rear speaker, microphone and USB-port; that is what you\'ll find in this module.</string>

    <!-- Vibration motor -->
    <string name="vibrator_test_title">Vibration motor</string>
    <string name="vibrator_test_description">This will test if the vibration motor on your Fairphone is working. Once the test starts the vibration motor will start pulsing.</string>
    <string name="vibrator_test_finish_question">The vibration motor should pulse now. Is the vibration motor working?</string>

    <!-- Rear speaker -->
    <string name="rear_speaker_test_title">Rear speaker</string>
    <string name="rear_speaker_test_description">This test checks if the rear speaker is working. Press \'start test\' and make sure you hear the music playing.</string>
    <string name="rear_speaker_test_finish_question">You should hear some music playing. Is the rear speaker working?</string>

