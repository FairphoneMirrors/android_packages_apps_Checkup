    <string name="camera_module_name">Camera</string>
    <string name="camera_module_description">The camera module contains the rear-facing camera and the flashlight.</string>

    <!-- Rear camera -->
    <string name="camera_test_title">Rear camera</string>
    <string name="camera_test_description">This test checks if the camera is working. It will display a preview of the camera picture. If you see the preview, then the camera is working correctly.</string>

