    <!-- Top module -->
    <string name="top_module_name">Top module</string>
    <string name="top_module_description">The top module contains the headphone jack, front camera and more.</string>

    <!-- Headphone jack -->
    <string name="headphone_jack_test_title">Headphone jack</string>
    <string name="headphone_jack_test_description">This test checks if the headphone jack is working properly. Press \'start test\' and plug and unplug your headphones a few times.</string>
